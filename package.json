{
  "name": "lgtmeme",
  "version": "0.0.1",
  "description": "u wot m8",
  "main": "index.js",
  "author": "Oliver Zheng <gitcommits@oliverzheng.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:chrome": "webpack --config chrome.webpack.config.js",
    "build:graphql": "apollo-codegen introspect-schema ./src/schema.graphql --output schema.json && apollo-codegen generate ./src/chrome-extension/**/*.js --schema schema.json --target flow --add-typename --output ./src/chrome-extension/graphql.js && prettier-eslint 'src/**/graphql.js' --prettier-last --write",
    "build:lambdas": "netlify-lambda build src/server/lambdas/",
    "precommit": "flow && lint-staged",
    "flow:watch": "flow-watch",
    "format": "prettier-eslint 'src/**/*.js' --prettier-last --write",
    "jest:watch": "jest --watch",
    "types:install": "flow-typed install",
    "types:update": "flow-typed update",
    "server:debug": "babel-node --inspect ./src/server/index.js",
    "lambdas:debug": "netlify-lambda serve src/server/lambdas/",
    "file-server": "babel-node ./src/scripts/startFileServer",
    "setup:mockgoose": "babel-node ./src/scripts/ensureMockMongo.js",
    "create-meme": "babel-node ./src/scripts/createMeme.js",
    "pretest": "yarn setup:mockgoose",
    "test:flow": "flow",
    "test:lint": "eslint 'src/**/*.js' --cache --max-warnings=0 --format junit -o reports/junit/eslint/js-lint-results.xml",
    "test:jest": "JEST_JUNIT_OUTPUT=reports/junit/jest/js-jest-results.xml jest",
    "test": "yarn test:flow && yarn test:lint && yarn test:jest"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testPathIgnorePatterns": [
      "/__mocks__/"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-eslint --prettier-last --write",
      "eslint --cache --max-warnings=0",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.49",
    "apollo-boost": "^0.1.7",
    "apollo-server-express": "^1.3.6",
    "apollo-server-lambda": "^1.3.6",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "flow-aws-lambda": "^1.0.3",
    "fs-extra": "^6.0.1",
    "graphql": "^0.13.2",
    "graphql-relay": "^0.5.5",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.2",
    "image-size": "^0.6.2",
    "invariant": "^2.2.4",
    "mongodb": "^3.1.0-beta4",
    "mongoose": "^4.13",
    "nullthrows": "^1.0.1",
    "s3": "^4.4.0",
    "tmp": "^0.0.33",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/node": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-flow": "^7.0.0-beta.49",
    "apollo-codegen": "^0.19.1",
    "app-root-path": "^2.0.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.4.0",
    "flow-watch": "^1.1.3",
    "flowtype-loader": "^0.2.2",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "jest-junit": "^5.0.0",
    "lint-staged": "^7.1.3",
    "mkdirp": "^0.5.1",
    "mongodb-memory-server": "^1.8.0",
    "netlify-lambda": "^0.4.0",
    "prettier": "1.12.1",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1"
  }
}
